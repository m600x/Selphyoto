{
  "name": "selphyoto",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "test": "bun test tests/unit/ tests/integration/",
    "test:watch": "bun test --watch tests/unit/ tests/integration/",
    "test:coverage": "bun test --coverage tests/unit/ tests/integration/",
    "test:e2e": "playwright test --config tests/e2e/playwright.config.ts",
    "test:all": "bun test tests/unit/ tests/integration/ && playwright test --config tests/e2e/playwright.config.ts"
  },
  "dependencies": {
    "fabric": "^7.2.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@happy-dom/global-registrator": "^20.6.3",
    "@playwright/test": "^1.58.2",
    "@types/bun": "^1.3.9",
    "eslint": "^10.0.0",
    "fake-indexeddb": "^6.2.5",
    "globals": "^17.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1"
  },
  "overrides": {
    "minimatch": "10.2.1"
  }
}
